{
  "name": "Trello Cards + Notifications",
  "version": "1.3.1",
  "description": "Lists the specified user assigned cards from all Trello boards.",
  "browser_action": {
    "popup": "popup.html",
    "default_icon" : "icon48.png"
  },
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [
	  "http://trello.com/*", 
	  "https://trello.com/*"    
	]
}